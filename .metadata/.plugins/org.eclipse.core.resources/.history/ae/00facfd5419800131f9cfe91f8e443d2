package hw2;
import java.util.*;

public class Process implements Comparable{
	
	float arrivalTime;
	float expectedRT;
	int priority;
	
	public Process (){
		Random rGen = new Random();
		arrivalTime = rGen.nextFloat() * 99;
		expectedRT = (float) (.1 + rGen.nextFloat() * 9.9);
		priority = 1 + rGen.nextInt(3);
	}

	@Override
	public int compareTo(Object o) {
		// TODO Auto-generated method stub
		Process compto = (Process) o;
		if (this.arrivalTime == compto.arrivalTime) return 0;
		else if(this.arrivalTime > compto.arrivalTime) return 1;
		else return -1;
		//return this.arrivalTime - compto.arrivalTime;
	}

}
